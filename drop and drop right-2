function drop(array,count=1){
arr=array
for(let i=0;i<count&&arr[i]!='\0';i++){
  arr.shift()
}
return arr;
}

function dropRight(array,count=1){
  arr=array
  diff=arr.length-count;
  arr.length=diff
  return arr
}



arr1=drop([1,2,3,4],2)

console.log(arr1)

arr2=dropRight([1,2,3,4],2)

console.log(arr2)
